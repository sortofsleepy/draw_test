{
  "extends": "@tsconfig/svelte/tsconfig.json",

  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Node",
    "jsx": "preserve",
    "noEmit": true,
    "importsNotUsedAsValues": "preserve",
    "types": ["svelte","snowpack-env"]
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
